int main () {
	
	int a = 0;
	int b = 1;

	if (a == (b-1)) {
		while (a < (b+4)) {
			printf("Iteration.\n");
			a = a + 1;
		}
	}
	else if (a == 0) {
		printf("Ceci ne devrait pas arriver.\n");
		a = b;
	}
	else {
		printf("Ceci ne devrait pas arriver.\n");
	}


	if (a == 0) {
		a = b;
	}
	else {
		printf("Ceci ne devrait pas arriver.\n");
	}


	if (a == 0) {
		printf("Coucou.\n");
	}

	return 0;
}